h1= main_title

each paragraph in description
    p!= paragraph 

each section in sections
  .preview
    h2= section.section_title

    h3 Preview
    p!= component(slug, section.data)

    h3 HTML code
    pre.code= htmlFormat(component(slug, section.data))

    h3 Component call
    pre.code
      | component('#{slug}', {as below})
      .component-data
        | #{jsonFormat(section.data)}

